{
	"plugins": ["testing-library", "jest-dom"],
	"extends": [
		"react-app",
		"react-app/jest",
		"plugin:testing-library/react",
		"plugin:jest-dom/recommended"
	],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "warn",
        "curly":"warn"
	}
}
